odule.exports = function frameguard(options) {\n    var headerValue = getHeaderValueFromOptions(options);\n    return function frameguard(_req, res, next) {\n        res.setHeader('X-Frame-Options', headerValue);\n        next();\n    };\n};\n"},"lineCount":null}},"hash":"32e4fa0039ddcecbdcc21311172b3414","cacheData":{"env":{}},"id":59}